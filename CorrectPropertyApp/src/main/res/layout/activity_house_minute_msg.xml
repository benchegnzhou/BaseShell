<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_house_minute_msg"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="china.test.meui.HouseMinuteMsgActivity">

    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/service_orders_bar"
        android:layout_width="match_parent"
        android:layout_height="156px"
        android:background="@color/apptheme">
        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/iv_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:paddingLeft="36px"
            android:paddingRight="60px">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:src="@drawable/app_common_title_back" />

        </com.zhy.autolayout.AutoRelativeLayout>

        <TextView
            android:id="@+id/tv_title"
            style="@style/app_top_common_title_stytle"
            android:text="房屋详情" />

    </com.zhy.autolayout.AutoRelativeLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="这里是房屋的详细信息页面"
        android:layout_centerInParent="true"
        />


</com.zhy.autolayout.AutoRelativeLayout>
